import { AfterContentInit, EventEmitter, OnInit, QueryList } from '@angular/core';
import { DropdownItemComponent } from './dropdown.item.component';
export declare class ButtonSplitDropdownComponent implements OnInit, AfterContentInit {
    label: string;
    type: string;
    size: string;
    cClass: string;
    dropdownItemData: any[];
    elementId: any;
    btnStyleClass: string;
    btnDropdownStyle: string;
    btnGroupStyleClass: string;
    onClick: EventEmitter<any>;
    dropdownItemRef: QueryList<DropdownItemComponent>;
    constructor();
    ngOnInit(): void;
    ngAfterContentInit(): void;
    createConfig(): void;
    createDropdownItemConfig(): void;
    itemClick(event: any, itemData: any): void;
    btnClick(clickData: any): void;
}
