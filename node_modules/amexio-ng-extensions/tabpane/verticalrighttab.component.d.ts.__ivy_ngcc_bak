import { AfterContentInit, AfterViewInit, OnInit, QueryList } from '@angular/core';
import { TabComponent } from './tabpill.component';
export declare class VerticalRightTabPaneComponent implements OnInit, AfterViewInit, AfterContentInit {
    tabs: TabComponent[];
    tapPosition: string;
    verticalText: boolean;
    tabwidth: string;
    cClass: string;
    queryTabs: QueryList<TabComponent>;
    elementId: string;
    constructor();
    ngOnInit(): void;
    ngAfterViewInit(): void;
    ngAfterContentInit(): void;
    activateTab(tabId: string): void;
}
