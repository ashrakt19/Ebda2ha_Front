import { EventEmitter, OnInit, QueryList } from '@angular/core';
import { StepBlockComponent } from "./step-block";
export declare class StepsComponent implements OnInit {
    showIndex: boolean;
    showIcon: boolean;
    showBlockBox: boolean;
    onBlockClick: EventEmitter<any>;
    stepBlocks: QueryList<StepBlockComponent>;
    stepBlockArray: StepBlockComponent[];
    constructor();
    onClick(event: any): void;
    ngAfterContentInit(): void;
    ngOnInit(): void;
}
